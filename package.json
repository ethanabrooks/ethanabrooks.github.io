{
  "name": "ethanabrooks.github.io",
  "version": "1.0.0",
  "description": "My personal website",
  "type": "module",
  "engines": {
    "node": ">=18.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "transformers": {
    "*.bib": [
      "@parcel/transformer-raw"
    ],
    "*.md": [
      "@parcel/transformer-raw"
    ]
  },
  "scripts": {
    "rescript": "rescript build -with-deps",
    "rescript-w": "rescript build -with-deps -w",
    "tailwind": "tailwindcss -i ./src/input.css -o ./lib/output.css",
    "tailwind-w": "yarn tailwind -w",
    "parcel-build": "parcel build index.html",
    "parcel": "parcel index.html",
    "clean": "rescript clean && rm -rf dist && rm -rf node_modules",
    "serve": "http-server dist -p 8080",
    "build": "yarn rescript && yarn tailwind && yarn parcel-build",
    "postinstall": "patch-package"
  },
  "author": "Ethan Brooks",
  "license": "MIT",
  "dependencies": {
    "@citation-js/plugin-bibtex": "^0.7.2",
    "@glennsl/bs-json": "^5.0.4",
    "@rescript/core": "*",
    "citation-js": "^0.7.4",
    "express": "^4.18.2",
    "marked": "^11.1.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rescript": "^10.1.4"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.10.3",
    "@parcel/transformer-inline-string": "2.10.3",
    "@rescript/react": "^0.11.0",
    "@types/node": "^20.5.0",
    "bs-fetch": "^0.6.2",
    "decco": "^1.6.0",
    "http-server": "^14.1.1",
    "parcel": "^2.10.3",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.0",
    "prisma": "^5.1.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  }
}
