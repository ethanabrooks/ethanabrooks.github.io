{
  "name": "runs",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "bsb -clean-world",
    "format": "prettier -w **/*.css **/*.html **/*.json **/*.scss src/**/*.js *.js",
    "dev": "concurrently 'node server.js dev' 'yarn dev:build' 'yarn dev:bundle' 'yarn dev:css'",
    "dev:build": "bsb -make-world -w -ws _",
    "dev:bundle": "esbuild --loader:.bib=text --loader:.html=text --define:process.version=\\\"v14.16.0\\\" --define:process.env.NODE_ENV=\\\"production\\\"  --serve=localhost:8001 --bundle src/Index.bs.js --external:react --external:react-dom",
    "dev:css": "postcss src/main.css -o dev/main.css",
    "predeploy": "concurrently 'yarn predeploy:bundle' 'yarn predeploy:css' 'yarn predeploy:static'",
    "predeploy:build": "concurrently 'yarn prod:bundle' 'yarn prod:css'",
    "predeploy:bundle": "bsb -make-world && esbuild --loader:.bib=text --loader:.html=text --define:process.version=\\\"v14.16.0\\\" --bundle src/Index.bs.js --external:react --external:react-dom --minify --outfile=dist/main.js",
    "predeploy:css": "NODE_ENV=production postcss src/main.css -o dist/main.css",
    "predeploy:static": "rsync -av static/ dist/",
    "deploy": "gh-pages --dist 'dist' --branch 'gh-pages'"
  },
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "bs-platform": "^9.0.1",
    "bs-webapi": "^0.19.1",
    "citation-js": "^0.5.1",
    "decco": "^1.5.0",
    "gh-pages": "^3.2.3",
    "html-react-parser": "^1.4.0",
    "jekyll": "^3.0.0-beta1",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "concurrently": "^5.3.0",
    "esbuild": "^0.13.12",
    "node-sass": "^6.0.0",
    "reanalyze": "^2.17.0",
    "reason-react": "^0.9.1"
  }
}
